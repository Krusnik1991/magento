<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Редактированный</title>
    </head>
    <referenceBlock name="newmodule_index_index" display="false" />
    <referenceBlock name="header.links">
        <block class="Magento\Framework\View\Element\Html\Link" name="custom-link01">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">custom link1</argument>
                <argument name="path" xsi:type="string" translate="true">customlink1</argument>
            </arguments>
        </block>
        <block class="Magento\Framework\View\Element\Html\Link" name="custom-link02">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">custom link2</argument>
                <argument name="path" xsi:type="string" translate="true">customlink2</argument>
            </arguments>
        </block>
        <block class="Magento\Framework\View\Element\Html\Link" name="custom-link03">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">custom link3</argument>
                <argument name="path" xsi:type="string" translate="true">customlink3</argument>
            </arguments>
        </block>
    </referenceBlock>
    <referenceContainer name="content.top">
        <referenceBlock class="Shellpea\Newmodule\Block\Coblock" name="block.first" template="Shellpea_Newmodule::tempoblock.phtml">
            <arguments>
                <argument name="css_tempoblock" xsi:type="string">shellpea-tempoblock</argument>
            </arguments>
        </referenceBlock>
        <block class="Shellpea\Newmodule\Block\Flipcard" name="flipcard" template="Shellpea_Newmodule::flipcard.phtml">
        </block>
    </referenceContainer>
</page>
